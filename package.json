{
	"name": "commerce-product-management",
	"version": "1.0.0",
	"description": "Commerce Product Management",
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-react": "^7.7.4",
		"copy-webpack-plugin": "4.6.0",
		"eslint": "^7.1.0",
		"eslint-config-liferay": "^20.0.1",
		"eslint-plugin-liferay": "0.0.1",
		"liferay-npm-build-support": "^2.18.2",
		"liferay-npm-bundler": "^2.18.2",
		"prettier": "2.0.4",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1"
	},
	"dependencies": {
		"@clayui/button": "^3.6.0",
		"@clayui/form": "^3.14.4",
		"@clayui/loading-indicator": "^3.2.0",
		"@clayui/management-toolbar": "^3.3.0",
		"@clayui/pagination-bar": "^3.25.1",
		"@clayui/table": "^3.1.0",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"use-debounce": "^3.4.2"
	},
	"scripts": {
		"build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
		"copy-assets": "lnbs-copy-assets",
		"translate": "lnbs-translate",
		"deploy": "npm run build && lnbs-deploy",
		"lint": "eslint --fix src",
		"prettier": "prettier --write src/",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "commerce",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": true,
		"javax.portlet.name": "commerceproductmanagement",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.resource-bundle": "content.Language"
	},
	"main": "index.js"
}
